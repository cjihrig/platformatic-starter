{
  "name": "platformatic-starter",
  "version": "0.1.0",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npx platformatic db compile",
    "dev": "concurrently 'npm:migrate' 'npm:start'",
    "format": "prettier --write \"**/*.ts\" \"*.ts\"",
    "postinstall": "npx husky install",
    "lint": "eslint \"**/*.ts\" \"*.ts\" --fix",
    "migrate": "npx platformatic db migrate",
    "service": "npx platformatic service start",
    "start": "npx platformatic db start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@platformatic/sql-graphql": "0.8.0",
    "@platformatic/sql-mapper": "0.8.0",
    "ajv": "8.11.2",
    "fastify": "4.10.2",
    "fluent-json-schema": "4.0.0",
    "platformatic": "0.8.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "concurrently": "7.6.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-xo": "0.43.1",
    "eslint-config-xo-typescript": "0.55.0",
    "husky": "8.0.2",
    "nano-staged": "0.8.0",
    "prettier": "2.8.0",
    "sort-package-json": "2.1.0"
  },
  "nano-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write",
    "package.json": "sort-package-json"
  }
}
