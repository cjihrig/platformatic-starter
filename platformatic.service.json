{
  "$schema": "./platformatic.db.schema.json",
  "server": {
    "logger": {
      "level": "{PLT_SERVER_LOGGER_LEVEL}"
    },
    "hostname": "{PLT_SERVER_HOSTNAME}",
    "port": "{PORT}",
    "healthCheck": {
      "interval": 2000
    }
  },
  "plugin": {
    "path": "micro.ts",
    "options": {
      "hello": "microservices"
    },
    "typescript": {
      "outDir": "dist"
    }
  }
}
